{
    "name": ":vendor/:package_name",
    "description": ":package_description",
    "keywords": [":vendor", ":package_name"],
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name", "email": ":author_email"
        }
    ],
    "require": {
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.5",
        "squizlabs/php_codesniffer": "~2.7"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    }
}
